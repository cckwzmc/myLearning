<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.toney.istyle.dao.ProductClickDao">
	<resultMap id="productClickResult" type="productClickModule">
		<id column="PID" property="pid" />
		<result column="TOTAL_CLICK" property="totalClick" />
		<result column="TRANSFER_NUMBER" property="transferNumber" />
		<result column="LIKE_NUMBER" property="likeNumber" />
		<result column="COMMENT_NUMBER" property="commentNumber" />
		<result column="RECOMMEND_NUMBER" property="recommendNumber" />
	</resultMap>
	<sql id="Base_Column_List">
		PID, TOTAL_CLICK, TRANSFER_NUMBER, LIKE_NUMBER, COMMENT_NUMBER, RECOMMEND_NUMBER
	</sql>
	<select id="selectById" resultMap="productClickResult" parameterType="java.lang.Long">
		select
		<include refid="Base_Column_List" />
		from T_PRODUCT_CLICK
		where PID = #{pid}
	</select>
	<delete id="deleteById" parameterType="java.lang.Long">
		delete from T_PRODUCT_CLICK
		where PID = #{pid}
	</delete>
	<insert id="insert" parameterType="com.toney.istyle.module.ProductClickModule">
		insert into T_PRODUCT_CLICK (PID,TOTAL_CLICK, TRANSFER_NUMBER,
		LIKE_NUMBER, COMMENT_NUMBER, RECOMMEND_NUMBER
		)
		values
		(#{pid},#{totalClick}, #{transferNumber},
		#{likeNumber}, #{commentNumber}, #{recommendNumber}
		)
	</insert>
	<update id="updateById" parameterType="com.toney.istyle.module.ProductClickModule"></update>
</mapper>