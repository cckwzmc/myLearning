<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
		<!-- JdbcType enumeration. Most common are: NULL, VARCHAR and OTHER -->
		<setting name="jdbcTypeForNull" value="NULL" />	
	</settings>
	<typeAliases>
		<typeAlias alias="platformModule" type="com.toney.istyle.model.PlatformModule" />
		<typeAlias alias="productPicModule" type="com.toney.istyle.model.ProductPictureModule" />
		<typeAlias alias="productModule" type="com.toney.istyle.model.ProductModule" />
		<typeAlias alias="userLoginModule" type="com.toney.istyle.model.UserLoginModule" />
		<typeAlias alias="userModule" type="com.toney.istyle.model.UserModule" />
		<typeAlias alias="productClickModule" type="com.toney.istyle.model.ProductClickModule" />
		<typeAlias alias="categoryModule" type="com.toney.istyle.model.CategoryModule" />
	</typeAliases>
	<!-- <mappers> <mapper resource="sqlmaps/UserSQL.xml" /> </mappers> -->
</configuration>