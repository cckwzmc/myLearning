<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
	default-lazy-init="true">

    <bean id="mocksFactory" name="addressManageFacade" class="com.xiu.portal.test.MocksFactory">
        <property name="type" value="com.xiu.uuc.facade.AddressManageFacade"/>
    </bean>
    <bean id="refundItemFlowServiceMock" name="refundItemFlowService" class="com.xiu.portal.test.MocksFactory">
        <property name="type" value="com.xiu.tc.orders.dointerface.RefundItemFlowService"/>
    </bean>
    
    <bean id="userManageFacadeMock" name="userManageFacade" class="com.xiu.portal.test.MocksFactory">
        <property name="type" value="com.xiu.uuc.facade.UserManageFacade"/>
    </bean>
    
    <bean id="bizOrderQueryMock" name="bizOrderQueryServive" class="com.xiu.portal.test.MocksFactory">
        <property name="type" value="com.xiu.tc.orders.dointerface.BizOrderQueryServive"/>
    </bean>
    
    <bean id="orderDetailQueryServiceMock" name="orderDetailQueryService" class="com.xiu.portal.test.MocksFactory">
        <property name="type" value="com.xiu.tc.orders.dointerface.OrderDetailQueryService"/>
